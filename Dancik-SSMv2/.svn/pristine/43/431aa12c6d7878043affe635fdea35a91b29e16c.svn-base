}

input[type=text], textarea {
    text-transform: none;
}


input[placeholder] {
    font-style: italic;
}
input[placeholder].editable {
    font-style: normal;
}
input:disabled{background-color:#fff !important; background-image:none !important; border-color:#eee !important; color:#555;}

button, input[type=submit], input[type=button] {
    font-weight: 700;
    height:34px;
}
textarea {
	padding-top: 4px;
	padding-bottom: 4px;
}
input[type=text], select, textarea {
    font-size: 10pt;
    padding-left:5px;
    padding-right:5px;
    border:1px solid #7F9DB9;
    border-radius: 4px;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
    border-bottom-left-radius: 4px;
    border-bottom-right-radius: 4px;
    -moz-border-radius-bottomleft: 4px;
    -moz-border-radius-bottomright: 4px;
    -webkit-border-bottom-left-radius: 4px;
    -webkit-border-bottom-right-radius: 4px;
}
input[type=text], select {
    height: 32px;
    line-height: 32px;
    background-image: url("../images/bkgds/gradients.png");
    background-position: center -261px;
    background-repeat: repeat-x;
    /*font-size:11pt;*/
   font-size:10pt;
}


option {
	height:32px;
	line-height:32px;
}
.upper{text-transform: uppercase;	}
input[type=text].upper {
	text-transform: uppercase;	
}

.bottom-padding-footer{
